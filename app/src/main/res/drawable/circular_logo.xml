                android:height="48dp" />
        </shape>
    </item>
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape>
            <solid android:color="@android:color/white" />
            <corners android:radius="50dp" />
            <gradient
                android:angle="45"
                android:endColor="@color/manchester_blue"
                android:startColor="@color/manchester_purple" />
        </shape>
    </item>
